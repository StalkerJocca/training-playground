pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
	parameters {
		string(name: 'NAME', defaultvalue: 'Joao')
		string(name: 'AGE', defaultvalue: '21')
	}
    stages {
        stage('build') {
            steps {
                echo "$NAME"
                echo "$AGE"
            }
        }
    }
}
